{
  "name": "workflowr",
  "description": "This is a clone of [workflowy](https://www.workflowy.com) written using MongoDB, Express, Node, and React.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "babel src/server --out-dir dist/server --presets=env",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "start": "nodemon dist/server/start.js",
    "watch:client": "webpack --watch",
    "watch:server": "npm run build -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "eslint": ">=3.14.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "webpack": "^3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danmakenoise/workflowr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danmakenoise/workflowr/issues"
  },
  "homepage": "https://github.com/danmakenoise/workflowr#readme"
}
