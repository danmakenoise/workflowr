{
  "name": "workflowr",
  "description": "This is a clone of [workflowy](https://www.workflowy.com) written using MongoDB, Express, Node, and React.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "babel src/server --out-dir dist/server --presets=env",
    "clean": "rm -rf dist",
    "start": "nodemon dist/server/start.js",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  },
